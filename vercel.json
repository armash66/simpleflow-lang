{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/web/backend/runner/src/main/resources/static/$1"
        }
    ],
    "functions": {
        "api/**/*.js": {
            "includeFiles": "{jre/**,simpleflow-lang/*.jar}",
            "memory": 1024,
            "maxDuration": 15
        }
    }
}