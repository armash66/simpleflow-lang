# ----------------------------
# Cell (single structure) demo
# ----------------------------
store c = @(1, 2, 3)
show c[1]
show length(c)

c[2] = 99
show c[2]

c["name"] = "mylang"
show c["name"]

# ----------------------------
# Loop with cell
# ----------------------------
loop (store i = 1; i <= length(c); i = i + 1) {
  show c[i]
}

exit
